{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MariaDbConnection": "Server=mariadb_new;Port=3306;Database=Kartverket;User={DB_USER};Password={DB_PASSWORD}"
  },
  "EmailSettings": {
    "SmtpServer": "{SMTP_SERVER}",
    "Port": "{SMTP_PORT}",
    "Username": "{SMTP_USERNAME}",
    "Password": "{SMTP_PASSWORD}",
    "FromEmail": "{SMTP_FROM_EMAIL}"
  }
}